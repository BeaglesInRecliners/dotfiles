
[alias]
  hist = log --color --pretty=format:\"%C(yellow)%h%C(reset) %C(cyan)%<(25)[%an]%C(reset) %s%C(bold red)%d%C(reset) %C(green)%ad%C(reset)\" --relative-date --decorate
  unstage = reset HEAD --
  restore = checkout --
  cn = commit --no-verify
  co = checkout
  praise = blame
  visualise = !gitk
  graph = log --color --graph --pretty=format:\"%h | %ad | %an | %s%d\" --date=short
  unpushed = log --branches --not --remotes --oneline --decorate

